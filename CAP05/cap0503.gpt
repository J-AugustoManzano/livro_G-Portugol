algoritmo TABELA3;
variáveis  
  I, J : inteiro;
  NOTA : matriz[8][4] de reais;
fim-variáveis
início
  imprima("Leitura das Notas");
  imprima(" ");
  para I de 0 até 7 faça 
    imprima("Notas do ", I + 1, "o. aluno:");
    para J de 0 até 3 faça
      NOTA[I][J] := leia();
    fim-para
    imprima(" ");
  fim-para
  imprima(" ");
  imprima("Apresentacao das Notas");
  para I de 0 até 7 faça
    imprima(I + 1, "o. aluno:");
    para J de 0 até 3 faça
      imprima(NOTA[I][J]);
    fim-para
  fim-para
fim
